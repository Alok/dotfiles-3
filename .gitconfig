[user]
    name = Ankur Dave
    email = ankurdave@gmail.com
[alias]
    st = status
    glog = log --oneline --decorate --graph
    slog = log --oneline --decorate --graph --simplify-by-decoration
[color]
    ui = auto
[core]
    pager = less -+$LESS -FRX
    excludesfile = ~/.gitignore
[rerere]
    enabled = 1
[receive]
    denyCurrentBranch = warn
